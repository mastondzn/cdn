{
    "name": "@mastondzn/cdn",
    "type": "module",
    "version": "0.0.1",
    "private": true,
    "description": "Personal image/file uploader, built with Cloudflare Workers & R2.",
    "author": {
        "name": "Maston",
        "url": "https://github.com/mastondzn"
    },
    "license": "GPL-3.0-or-later",
    "repository": {
        "type": "git",
        "url": "https://github.com/mastondzn/cdn.git"
    },
    "scripts": {
        "deploy": "wrangler deploy",
        "dev": "wrangler dev --port 3002",
        "start": "pnpm dev",
        "format": "prettier --write \"**/*.{ts,tsx,md,json,js,mjs,cjs,yml,yaml}\"",
        "format:check": "prettier -c \"**/*.{ts,tsx,md,json,js,mjs,cjs,yml,yaml}\"",
        "lint": "eslint .",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "hono": "^4.7.4",
        "mrmime": "^2.0.1",
        "nanoid": "^5.1.3"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20250312.0",
        "@mastondzn/eslint": "^1.2.0",
        "eslint": "^9.22.0",
        "prettier": "^3.5.3",
        "typescript": "^5.8.2",
        "wrangler": "^4.0.0"
    }
}
