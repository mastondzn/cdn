{
    "name": "@mastondzn/cdn",
    "type": "module",
    "version": "0.0.1",
    "private": true,
    "description": "Personal image/file uploader, built with Cloudflare Workers & R2.",
    "author": {
        "name": "Maston",
        "url": "https://github.com/mastondzn"
    },
    "license": "GPL-3.0-or-later",
    "repository": {
        "type": "git",
        "url": "https://github.com/mastondzn/cdn.git"
    },
    "scripts": {
        "deploy": "wrangler deploy",
        "dev": "wrangler dev --port 3002 --local",
        "start": "wrangler dev --port 3002 --local",
        "format": "prettier --write \"**/*.{ts,tsx,md,json,js,mjs,cjs,yml,yaml}\"",
        "format:check": "prettier -c \"**/*.{ts,tsx,md,json,js,mjs,cjs,yml,yaml}\"",
        "lint": "eslint .",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "hono": "^4.6.7",
        "mrmime": "^2.0.0",
        "nanoid": "^5.0.7"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20241022.0",
        "@mastondzn/eslint": "^0.10.0",
        "eslint": "^8.57.0",
        "prettier": "^3.3.3",
        "typescript": "^5.6.3",
        "wrangler": "^3.83.0"
    }
}
